{
  "name": "iphone-to-pixel",
  "version": "0.0.0",
  "description": "Convert iOS media files for Pixel compatibility",
  "type": "module",
  "bin": "./dist/index.js",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "start": "bun run dist/index.js",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "ios",
    "pixel",
    "heic",
    "hevc",
    "media",
    "converter"
  ],
  "author": "dna",
  "license": "MIT",
  "dependencies": {
    "commander": "^10.0.0",
    "execa": "^7.0.0",
    "kleur": "^4.1.5",
    "ora": "^6.1.2",
    "zod": "^3.20.2"
  },
  "devDependencies": {
    "@types/node": "^20.11.27",
    "tsup": "^6.6.3",
    "typescript": "^5.5.3"
  }
}
